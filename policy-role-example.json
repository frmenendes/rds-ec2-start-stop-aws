{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogStream",
                "logs:CreateLogGroup",
                "logs:PutLogEvents"
            ],
            "Resource": "arn:aws:logs:*:*:*"
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": [
                "ec2:DescribeInstances",
                "ec2:StartInstances",
                "rds:*",
                "rds:ListTagsForResource",
                "rds:DescribeDBInstances",
                "ec2:*",
                "ec2:StopInstances",
                "rds:StopDBInstance",
                "events:*",
                "rds:StartDBInstance"
            ],
            "Resource": "*"
        }
    ]
}